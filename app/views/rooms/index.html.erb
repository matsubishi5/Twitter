<div class="container">
    <div class="row">
        <div class="tweets-sidebar-left col-lg-2">
            <%= render "shared/sidebar-left" %>
        </div>
        <div class="col-lg-3">
            <table class="table">
                <tbody>
                    <h1>メッセージ</h1>
                    <% @anotherEntries.each do |e| %>
                    <tr>
                        <td>
                            <%= link_to room_path(e.room.id) do %>
                            <%= attachment_image_tag e.user, :profile_image, :size =>'30x30', class: "img-circle pull-left profile-thumb", fallback: "no_image1.jpg" %>
                            <%= e.user.name %>
                            <p><%= Message.find_by(id: e.room.message_ids.last).content %></p>
                            <% end %>
                        </td>
                    </tr>
                    <% end %>
                </tbody>
            </table>
        </div>
        <div class="col-lg-7">
        </div>
    </div>
</div>